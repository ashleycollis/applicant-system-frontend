    "knex": "^0.20.11",
    "morgan": "~1.9.1",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "mocha": "^7.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
